{
  "apps": [
    {
      "name": "foxdata",
      "cwd": "/var/nodejs/foxdata",
      "script": "main.js",
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "error_file": "/var/nodejs/log/foxdata_erroe.log",
      "out_file": "log/foxdata.log",
      "pid_file": "pids/foxdata.pid",
      "instances": 1,
      "min_uptime": "200s",
      "max_restarts": 10,
      "max_memory_restart": "1M",
      "cron_restart": "1 0 * * *",
      "watch": false,
      "merge_logs": true,
      "exec_interpreter": "node",
      "exec_mode": "fork",
      "autorestart": false,
      "vizion": false
    }
  ]
}